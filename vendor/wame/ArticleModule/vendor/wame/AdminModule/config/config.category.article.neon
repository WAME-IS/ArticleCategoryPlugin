services:
	- Wame\ArticleCategoryPlugin\Wame\ArticleModule\Wame\AdminModule\Forms\ICategoryTreeFormContainerFactory

	# Events
	- {class: Wame\ArticleCategoryPlugin\Wame\ArticleModule\Wame\AdminModule\Events\CategoryListener, tags: [run]}

	# Factories
	- Wame\ArticleCategoryPlugin\Forms\ICategoryTreeFormContainerFactory

	# Forms
	CreateArticleForm:
		setup:
			- addFormContainer(Wame\ArticleCategoryPlugin\Wame\ArticleModule\Wame\AdminModule\Forms\CategoryTreeContainer(), 'CategoryTreeFormContainer', 50)


entityModifier:
    traits:
    - {class: Wame\ArticleModule\Entities\ArticleEntity, trait: Wame\ArticleCategoryPlugin\Entities\Columns\Category}
    - {class: Wame\CategoryModule\Entities\CategoryEntity, trait: Wame\ArticleCategoryPlugin\Entities\Columns\Article}